<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="lee" class="com.test03.Student">
		<property name="name" value="이창진" />
		<property name="addr" value="충청남도 천안시" />
		<property name="age" value="27" />
	</bean>
	<bean id="hong" class="com.test03.Student">
		<property name="name" value="홍길동" />
		<property name="addr" value="충청남도 공주시" />
		<property name="age" value="23" />
	</bean>
	
	<bean id="mySchool" class="com.test03.School">
		<property name="grade" value="1" />
	</bean>
	
	<context:annotation-config />
</beans>
